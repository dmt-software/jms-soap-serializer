{
    "name": "dmt-software/jms-soap-serializer",
    "description": "JMS serialize visitor that (de)serializes SOAP requests/responses",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas de Mes",
            "email": "bas@dmt-software.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-intl" : "*",
        "ext-simplexml": "*",
        "jms/serializer": "^3.19"
    },
    "autoload": {
        "psr-4": {
            "DMT\\Soap\\Serializer\\": "src/"
        }
    },
    "require-dev": {
        "ext-soap" : "*",
        "phpunit/phpunit": "^7.2 || ^8.0"
    },
    "autoload-dev": {
        "psr-4": {
            "DMT\\Test\\Soap\\Serializer\\": "tests/"
        }
    }
}
